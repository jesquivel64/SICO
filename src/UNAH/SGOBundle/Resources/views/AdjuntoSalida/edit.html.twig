{% extends "UNAHSGOBundle::layout.html.twig" %}
{% block content %}
<aside class="span3">
  <nav class="well sidebar-nav">
    <ul class="nav nav-list">
      <li>
        <a href="{{ path('departamento') }}">Regresar a Oficios</a>
      <li>
          <a href="{{ path('salida_show', { 'id': documento.id }) }}">Regresar a {{ documento.tipo }} {{ documento.numero }}</a>
      </li>
    </ul>
  </nav>
</aside>
<section class="span6">
    <form action="{{ path('adjuntosalida_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    <fieldset>
      <legend>
        Editar Adjunto
      </legend>
      <input type="hidden" name="_method" value="PUT" />
      {{ form_widget(edit_form) }}
    </fieldset>
    <div class="form-actions">
      <button class="btn btn-primary" type="submit">
        Actualizar
      </button>
    </div>
  </form>
  <form action="{{ path('adjuntosalida_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
</section>
{% endblock %}
